{
  "name": "PKG_NAME",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "concurrently -n serve,watch \"serve public\" npm:watch",
    "build": "NODE_ENV=production browserify -t babelify -t envify -g uglifyify index.js | terser -co public/bundle.js",
    "watch": "NODE_ENV=development watchify -t babelify -t envify -d index.js -o public/bundle.js"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "d.el",
          "pragmaFrag": "d.JsxFragment",
          "throwIfNamespace": false
        }
      ]
    ]
  },
  "dependencies": {
    "@dominant/core": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.13",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "concurrently": "^5.3.0",
    "envify": "^4.1.0",
    "serve": "^11.3.2",
    "terser": "^3.17.0",
    "uglifyify": "^5.0.2",
    "watchify": "^4.0.0"
  },
  "publishConfig": {
    "access": "private"
  }
}
